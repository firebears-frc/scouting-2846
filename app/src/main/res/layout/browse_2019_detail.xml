    <TextView
        android:id="@+id/tele_hatch_lo"
        android:text="0"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/hatch_floor"
        android:layout_toRightOf="@id/thl_txt"/>

    <TextView
        android:id="@+id/thm_txt"
        android:text="@string/place_md"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/thl_txt"/>

    <TextView
        android:id="@+id/tele_hatch_md"
        android:text="0"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/thl_txt"
        android:layout_toRightOf="@id/thm_txt"/>

    <TextView
        android:id="@+id/thh_txt"
        android:text="@string/place_hi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/thm_txt"/>

    <TextView
        android:id="@+id/tele_hatch_hi"
        android:text="0"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/thm_txt"
        android:layout_toRightOf="@id/thh_txt"/>

    <TextView
        android:id="@+id/tele_cargo"
        android:text="@string/tele_cargo"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/thh_txt"/>

    <TextView
        android:id="@+id/cfh_txt"
        android:text="@string/from_human"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tele_cargo"/>

    <TextView
        android:id="@+id/cargo_human"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tele_cargo"
        android:layout_toRightOf="@id/cfh_txt"/>

    <TextView
        android:id="@+id/cff_txt"
        android:text="@string/from_floor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/cfh_txt"/>

    <TextView
        android:id="@+id/cargo_floor"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/cargo_human"
        android:layout_toRightOf="@id/cff_txt"/>

    <TextView
        android:id="@+id/tcs_txt"
        android:text="@string/cargo_ship"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/cff_txt"/>

    <TextView
        android:id="@+id/cargo_ship"
        android:text="0"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/cargo_floor"
        android:layout_toRightOf="@id/tcs_txt"/>

    <TextView
        android:id="@+id/tcl_txt"
        android:text="@string/place_lo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

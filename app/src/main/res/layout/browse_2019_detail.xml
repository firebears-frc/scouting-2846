    <TextView
        android:id="@+id/tele_cargo_lo"
        android:text="0"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/cargo_ship"
        android:layout_toRightOf="@id/tcl_txt"/>

    <TextView
        android:id="@+id/tcm_txt"
        android:text="@string/place_md"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tcl_txt"/>

    <TextView
        android:id="@+id/tele_cargo_md"
        android:text="0"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tcl_txt"
        android:layout_toRightOf="@id/tcm_txt"/>

    <TextView
        android:id="@+id/tch_txt"
        android:text="@string/place_hi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tcm_txt"/>

    <TextView
        android:id="@+id/tele_cargo_hi"
        android:text="0"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tcm_txt"
        android:layout_toRightOf="@id/tch_txt"/>

    <View
        style="@style/Divider"
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tch_txt"/>

    <TextView
        android:id="@+id/endgame"
        android:text="@string/endgame"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/divider2"/>

    <TextView
        android:id="@+id/cl1_txt"
        android:text="@string/climb_lvl_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/endgame"/>

    <TextView
        android:id="@+id/climb_lvl_1"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/endgame"
        android:layout_toRightOf="@id/cl1_txt"/>

    <TextView
        android:id="@+id/cl2_txt"
        android:text="@string/climb_lvl_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/cl1_txt"/>

    <TextView
        android:id="@+id/climb_lvl_2"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/climb_lvl_1"
        android:layout_toRightOf="@id/cl2_txt"/>

    <TextView
        android:id="@+id/cl3_txt"
        android:text="@string/climb_lvl_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/cl2_txt"/>

    <TextView
        android:id="@+id/climb_lvl_3"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/climb_lvl_2"
        android:layout_toRightOf="@id/cl3_txt"/>

    <TextView
        android:id="@+id/asl2_txt"
        android:text="@string/assist_lvl_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/cl3_txt"/>

    <TextView
        android:id="@+id/assist_lvl_2"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/climb_lvl_3"
        android:layout_toRightOf="@id/asl2_txt"/>

    <TextView
        android:id="@+id/asl3_txt"
        android:text="@string/assist_lvl_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/asl2_txt"/>

    <TextView
        android:id="@+id/assist_lvl_3"
        android:gravity="right"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/assist_lvl_2"
        android:layout_toRightOf="@id/asl3_txt"/>

    <View
        style="@style/Divider"
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/asl3_txt"/>

    <TextView
        android:id="@+id/notes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/divider3"/>

  </RelativeLayout>
</android.support.v4.widget.NestedScrollView>
</android.support.design.widget.CoordinatorLayout>
